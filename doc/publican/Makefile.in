@HAVE_PUBLICAN_TRUE@@HAVE_XSLTPROC_TRUE@	en-US/ProtocolSpec.xml en-US/ProtocolInterfaces.xml \
@HAVE_PUBLICAN_TRUE@@HAVE_XSLTPROC_TRUE@	en-US/WaylandClientAPI.xml en-US/WaylandServerAPI.xml

